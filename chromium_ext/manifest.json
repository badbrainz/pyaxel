{
   "name": "pyaxelWS",
   "version": "1.1.0",
   "minimum_chrome_version": "16",
   "manifest_version": 2,
   "description": "Download Accelerator for Chrome(Linux).",
   "update_url": "http://clients2.google.com/service/update2/crx",
   "background": {
      "scripts": [
         "js/io.js",
         "js/parseuri.js",
         "js/timer.js",
         "js/utils.js",
         "js/queue.js",
         "js/regex.js",
         "js/broadcaster.js",
         "js/storage.js",
         "js/jobqueue.js",
         "js/connection.js",
         "js/client.js",
         "js/badge.js",
         "js/background.js"
      ]
   },
   "options_page": "settings.html",
   "browser_action": {
      "default_icon": "images/19.png",
      "default_popup": "popup.html"
   },
   "icons": {
      "128": "images/128.png",
      "16": "images/16.png",
      "48": "images/48.png"
   },
   "permissions": [
      "https://*/*",
      "http://*/*",
      "ftp://*/*",
      "contextMenus",
      "notifications",
      "tabs"
      ]
}
